<?xml version="1.0"?>
<robot name="robotiq_hande_cleaned" xmlns:xacro="http://ros.org/wiki/xacro">

  <link name="robotiq_hande_coupler">
    <visual>
      <geometry>
        <mesh filename="meshes/io_coupler.dae"/>
      </geometry>
    </visual>
    <collision>
      <geometry>
        <mesh filename="meshes/io_coupler.dae"/>
      </geometry>
    </collision>
    <inertial>
      <mass value="0.86387"/>
      <origin xyz="0 0 0.025" rpy="0 0 0"/>
      <inertia
        ixx="0.00040846"
        ixy="0.0"
        ixz="0.0"
        iyy="0.00040846"
        iyz="0.0"
        izz="0.00045599"/>
    </inertial>
  </link>

  <joint name="robotiq_hande_base_joint" type="fixed">
    <origin xyz="0 0 0.011" rpy="0 0 0"/>
    <parent link="robotiq_hande_coupler"/>
    <child link="robotiq_hande_link"/>
  </joint>

  <link name="robotiq_hande_link">
    <visual>
      <geometry>
        <mesh filename="meshes/hande.dae" scale="1.0 1.0 1.0"/>
      </geometry>
    </visual>
    <collision>
      <geometry>
        <mesh filename="meshes/hande.dae" scale="1.0 1.0 1.0"/>
      </geometry>
    </collision>
  </link>

  <joint name="robotiq_hande_left_finger_joint" type="prismatic">
    <origin xyz="0 0 0.099" rpy="0 0 0"/>
    <parent link="robotiq_hande_link"/>
    <child link="robotiq_hande_left_finger"/>
    <axis xyz="1 0 0"/>
    <limit lower="0.0" upper="0.01" effort="1.0" velocity="0.01"/>
  </joint>

  <link name="robotiq_hande_left_finger">
    <visual>
      <geometry>
        <mesh filename="meshes/finger.dae" scale="1.0 1.0 1.0"/>
      </geometry>
    </visual>
    <collision>
      <geometry>
        <mesh filename="meshes/finger_collision.dae" scale="1.0 1.0 1.0"/>
      </geometry>
    </collision>
    <inertial>
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <mass value="0.03804"/>
        <inertia ixx="1E-9" ixy="0.0" ixz="0.0" iyy="1E-9" iyz="0.0" izz="1E-9"/>
      </inertial>
  </link>

  <joint name="robotiq_hande_right_finger_joint" type="prismatic">
    <origin xyz="0 0 0.099" rpy="0 0 0"/>
    <parent link="robotiq_hande_link"/>
    <child link="robotiq_hande_right_finger"/>
    <axis xyz="-1 0 0"/>
    <limit lower="0.0" upper="0.01" effort="1.0" velocity="0.01"/>
    <mimic joint="robotiq_hande_left_finger_joint" multiplier="1.0" offset="0.0"/>
  </joint>

  <link name="robotiq_hande_right_finger">
    <visual>
      <origin rpy="0 0 3.14159"/>
      <geometry>
        <mesh filename="meshes/finger.dae" scale="1.0 1.0 1.0"/>
      </geometry>
    </visual>
    <collision>
      <origin rpy="0 0 3.14159"/>
      <geometry>
        <mesh filename="meshes/finger_collision.dae" scale="1.0 1.0 1.0"/>
      </geometry>
    </collision>
    <inertial>
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <mass value="0.03804"/>
        <inertia ixx="1E-9" ixy="0.0" ixz="0.0" iyy="1E-9" iyz="0.0" izz="1E-9"/>
      </inertial>
  </link>

  <joint name="robotiq_hande_end_joint" type="fixed">
    <origin xyz="0 0 0.1455" rpy="0 0 0"/>
    <parent link="robotiq_hande_link"/>
    <child link="robotiq_hande_end"/>
  </joint>

  <link name="robotiq_hande_end"/>
</robot>

